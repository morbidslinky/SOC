<?xml version="1.0" encoding="utf-8"?>
<Scriptal xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Set Number Variable</Name>
  <Description>Stores a number to a user variable.</Description>
  <EventFunctionTemplate>|[1|NUMBER]| = |[2|NUMBER]|</EventFunctionTemplate>
  <Choices>
	<Choice>
	  <Name>User Variable</Name>
	  <Description>The custom variable that the value will be set to.</Description>
	  <AllowUIEdit>true</AllowUIEdit>
	  <AllowUserVariable>true</AllowUserVariable>
	  <AllowLiteral>false</AllowLiteral>
	</Choice>
	<Choice>
	  <Name>Number Value</Name>
	  <Description>The value to set to the User Variable.</Description>
	  <ChoosableValueSetsFilter>
	    <Key>Event Default Arguments</Key>
	  </ChoosableValueSetsFilter>
	  <AllowUIEdit>true</AllowUIEdit>
	  <AllowLiteral>true</AllowLiteral>
	  <AllowUserVariable>false</AllowUserVariable>
	  <Key>Number Literal</Key>
	  <Value xsi:type="LuaNumber" Number="1" />
	</Choice>
  </Choices>
</Scriptal>